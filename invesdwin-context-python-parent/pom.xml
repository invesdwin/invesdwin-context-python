			<name>GNU Lesser General Public License (LGPL), Version 3.0</name>
			<url>http://www.gnu.org/licenses/lgpl-3.0.html</url>
		</license>
	</licenses>


	<modules>
		<module>invesdwin-context-python-runtime-contract</module>
		<module>invesdwin-context-python-runtime-jython</module>
		<module>invesdwin-context-python-runtime-jep</module>
		<module>invesdwin-context-python-runtime-py4j</module>
		<module>invesdwin-context-python-runtime-libpythonclj</module>
		<module>invesdwin-context-python-runtime-python4j</module>
		<module>invesdwin-context-python-runtime-japyb</module>
	</modules>

	<properties>
		<project.version.invesdwin-context>1.0.4-SNAPSHOT</project.version.invesdwin-context>
		<project.version.invesdwin-context-integration>1.0.3-SNAPSHOT</project.version.invesdwin-context-integration>
		<project.version.invesdwin-context-python>${project.version}</project.version.invesdwin-context-python>

		<version.jython>2.7.3</version.jython>
		<version.jep>4.0.0</version.jep>
		<version.py4j>0.10.9.3</version.py4j>
		<version.libpython-clj>2.018</version.libpython-clj>
		<version.python4j>1.0.0-M2.1</version.python4j>
	</properties>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>de.invesdwin</groupId>
				<artifactId>invesdwin-context-parent</artifactId>
				<version>${project.version.invesdwin-context}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			<dependency>
				<groupId>de.invesdwin</groupId>
				<artifactId>invesdwin-context-integration-parent</artifactId>
				<version>${project.version.invesdwin-context-integration}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			<dependency>
				<groupId>de.invesdwin</groupId>
				<artifactId>invesdwin-context-python-runtime-contract</artifactId>
