				<artifactId>invesdwin-context-python-runtime-python4j</artifactId>
				<version>${project.version.invesdwin-context-python}</version>
			</dependency>
			<dependency>
				<groupId>de.invesdwin</groupId>
				<artifactId>invesdwin-context-python-runtime-japyb</artifactId>
				<version>${project.version.invesdwin-context-python}</version>
			</dependency>

			<dependency>
				<groupId>org.python</groupId>
				<artifactId>jython-slim</artifactId>
				<version>${version.jython}</version>
				<exclusions>
					<exclusion>
						<groupId>org.bouncycastle</groupId>
						<artifactId>bcpkix-jdk15on</artifactId>
					</exclusion>
					<exclusion>
						<groupId>org.bouncycastle</groupId>
						<artifactId>bcprov-jdk15on</artifactId>
					</exclusion>
					<exclusion>
						<groupId>javax.servlet</groupId>
