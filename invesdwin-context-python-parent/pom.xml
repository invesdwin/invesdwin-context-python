				<version>${version.py4j}</version>
			</dependency>
			<dependency>
				<groupId>clj-python</groupId>
				<artifactId>libpython-clj</artifactId>
				<version>${version.libpython-clj}</version>
			</dependency>
			<dependency>
				<groupId>org.nd4j</groupId>
				<artifactId>python4j-numpy</artifactId>
				<version>${version.python4j}</version>
			</dependency>
			<dependency>
				<groupId>org.nd4j</groupId>
				<artifactId>python4j-core</artifactId>
				<version>${version.python4j}</version>
			</dependency>
